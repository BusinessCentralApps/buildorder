{
  "type": "PTE",
  "templateUrl": "https://github.com/freddydk/AL-Go@PPP-split",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "development"
  ],
  "CICDPullRequestBranches": [
    "main",
    "development"
  ],
  "DoNotUseCdnForArtifacts": true,
  "useProjectDependencies": true,
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "runs-on": "ubuntu-latest",
  "gitHubRunner": "ubuntu-latest",
  "githubRunnerShell": "pwsh",
  "ALDoc": {
    "maxReleases": 0,
    "continuousDeployment": true,
    "excludeProjects": [
      "test"
    ]
  },
  "DeployToTest": {
    "EnvironmentType": "Test"
  },
  "DeliverToStorage": {
    "CreateContainerIfNotExist": true
  },
  "templateSha": "9e348b6352906506141d65ec402d54ccdde43002",
  "RepoVersion": "1.0"
}
