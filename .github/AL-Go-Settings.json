{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@preview",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "development"
  ],
  "CICDPullRequestBranches": [
    "main",
    "development"
  ],
  "DoNotUseCdnForArtifacts": true,
  "useProjectDependencies": true,
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "runs-on": "windows-latest",
  "shell": "powershell",
  "gitHubRunner": "windows-latest",
  "githubRunnerShell": "powershell",
  "ALDoc": {
    "maxReleases": 0,
    "continuousDeployment": true,
    "excludeProjects": [
      "test"
    ]
  },
  "DeployToTest": {
    "EnvironmentType": "Test",
    "runs-on": [
      "ubuntu-latest"
    ],
    "shell": "pwsh"
  },
  "DeliverToStorage": {
    "CreateContainerIfNotExist": true
  },
  "templateSha": "9c047553618e3eb937d33b03e4407354e3ef4531",
  "RepoVersion": "1.0",
  "artifact": "//24.0//first",
  "vsixFile": "latest",
  "customCodeCops": [
    "https://github.com/StefanMaron/BusinessCentral.LinterCop/releases/download/v0.30.0/BusinessCentral.LinterCop.current.dll"
  ]
}
