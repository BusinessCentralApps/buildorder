{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@preview",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "development"
  ],
  "CICDPullRequestBranches": [
    "main",
    "development"
  ],
  "useProjectDependencies": true,
  "useCompilerFolder": false,
  "doNotPublishApps": false,
  "runs-on": "windows-latest",
  "shell": "powershell",
  "gitHubRunner": "windows-latest",
  "githubRunnerShell": "pwsh",
  "ALDoc": {
    "maxReleases": 0,
    "continuousDeployment": true,
    "excludeProjects": [
      "test"
    ]
  },
  "generateDependencyArtifact": true,
  "DeployToTest": {
    "EnvironmentType": "Test",
    "runs-on": [
      "ubuntu-latest"
    ],
    "shell": "pwsh",
    "myproperty": "test"
  },
  "DeliverToStorage": {
    "CreateContainerIfNotExist": true
  },
  "templateSha": "becb26e1f895fb75145bbc10535afb19c09bff5c",
  "RepoVersion": "1.0",
  "artifact": "//24.0//first",
  "vsixFile": "latest",
  "customCodeCops": [
    "https://github.com/StefanMaron/BusinessCentral.LinterCop/releases/download/v0.30.0/BusinessCentral.LinterCop.current.dll"
  ],
  "customALGoSystemFiles": [
    {
      "Destination": ".AL-Go/",
      "Source": "https://raw.githubusercontent.com/freddydk/CustomALGoSystemFiles/main/.AL-Go/myDevEnv.ps1"
    },
    {
      "Destination": ".AL-Go/FakeNewBcContainer.ps1",
      "Source": "https://raw.githubusercontent.com/microsoft/BCApps/main/build/scripts/NewBcContainer.ps1"
    },
    {
      "Destination": ".github/",
      "Source": "https://github.com/freddydk/CustomALGoSystemFiles/archive/refs/heads/main.zip",
      "FileSpec": "*/.github/*",
      "Recurse": true
    }
  ],
  "AZURE_CREDENTIALSSecretName": "AzureCredentials"
}
