{
  "type": "PTE",
  "templateUrl": "https://github.com/freddydk/AL-Go@MODUS",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "development"
  ],
  "incrementalBuilds": {
    "enable": true
  },
  "CICDPullRequestBranches": [
    "main",
    "development"
  ],
  "conditionalSettings": [
    {
      "workflows": [
        "CICD"
      ],
      "settings": {
        "workflowconcurrency": [
          "group: ${{ github.workflow }}-${{ github.ref }}",
          "cancel-in-progress: true"
        ],
        "workflowschedule": "5 4 * * *"
      }
    },
    {
      "buildModes": [
        "Default"
      ],
      "settings": {
        "artifact": "https://bcartifacts-exdbf9fwegejdqak.b02.azurefd.net/sandbox/24.0.16410.18040/w1"
      }
    },
    {
      "buildModes": [
        "Clean"
      ],
      "settings": {
        "preprocessorSymbols": [
          "CLEAN"
        ]
      }
    },
    {
      "buildModes": [
        "NextMajor"
      ],
      "settings": {
        "artifact": "///w1/nextmajor/",
        "preprocessorSymbols": [
          "NEXTMAJOR",
          "TEST"
        ]
      }
    },
    {
      "buildModes": [
        "NextMinor"
      ],
      "settings": {
        "artifact": "///w1/nextminor/",
        "preprocessorSymbols": [
          "NEXTMAJOR",
          "TEST"
        ]
      }
    }
  ],
  "PreprocessorSymbols": [
    "ALL"
  ],
  "useProjectDependencies": true,
  "runs-on": "ubuntu-latest",
  "shell": "pwsh",
  "gitHubRunner": "windows-latest",
  "githubRunnerShell": "pwsh",
  "generateDependencyArtifact": true,
  "templateSha": "3462044f1437f5eab55091c30c2368b321cf85c2",
  "RepoVersion": "1.0"
}
