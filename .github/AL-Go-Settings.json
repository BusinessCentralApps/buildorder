{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@main",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "development"
  ],
  "defaultNewContainerParameters": {
    "isolation": "hyperv"
  },
  "incrementalBuilds": {
    "onPush": true,
    "onSchedule": false
  },
  "pullRequestTrigger": "pull_request",
  "CICDPullRequestBranches": [
    "main",
    "development"
  ],
  "conditionalSettings": [
    {
      "workflows": [
        "CICD"
      ],
      "settings": {
        "workflowconcurrency": [
          "group: ${{ github.workflow }}-${{ github.ref }}",
          "cancel-in-progress: true"
        ],
        "workflowschedule": {
          "cron": "45 * * * *"
        }
      }
    },
    {
      "buildModes": [
        "Default"
      ],
      "settings": {
        "artifact": "https://bcartifacts-exdbf9fwegejdqak.b02.azurefd.net/sandbox/24.0.16410.18040/w1"
      }
    },
    {
      "buildModes": [
        "Clean"
      ],
      "settings": {
        "preprocessorSymbols": [
          "CLEAN"
        ]
      }
    },
    {
      "buildModes": [
        "NextMajor"
      ],
      "settings": {
        "artifact": "///w1/nextmajor/",
        "preprocessorSymbols": [
          "NEXTMAJOR",
          "TEST"
        ]
      }
    },
    {
      "buildModes": [
        "NextMinor"
      ],
      "settings": {
        "artifact": "///w1/nextminor/",
        "preprocessorSymbols": [
          "NEXTMAJOR",
          "TEST"
        ]
      }
    }
  ],
  "PreprocessorSymbols": [
    "ALL"
  ],
  "useProjectDependencies": true,
  "runs-on": "ubuntu-latest",
  "shell": "pwsh",
  "gitHubRunner": "windows-latest",
  "githubRunnerShell": "pwsh",
  "generateDependencyArtifact": true,
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "templateSha": "6472dd494728483cee9b04dc7356fd6f2cc737b4",
  "RepoVersion": "1.0"
}
